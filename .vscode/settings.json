{
  "files.exclude": {
    "**/__pycache__": true,
    "**/.pytest_cache": true,
    "server/birdnet-api/venv": true,
    "server/express-api/node_modules": true,
    "server/express-api/dist": true,
    "birdnet-client/node_modules": true
  },
  "search.exclude": {
    "server/birdnet-api/venv": true,
    "server/express-api/node_modules": true,
    "server/express-api/dist": true,
    "birdnet-client/node_modules": true
  },
  "python.analysis.extraPaths": [
    "${workspaceFolder}/server/birdnet-api"
  ],
  "python.testing.pytestEnabled": false,
  "python.testing.unittestEnabled": false,
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.formatOnSave": true
}
