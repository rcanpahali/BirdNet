{
  "name": "birdnet",
  "private": true,
  "workspaces": [
    "birdnet-client",
    "server/express-api"
  ],
  "scripts": {
    "start": "npm-run-all --parallel birdnet-api express-api birdnet-client",
    "birdnet-api": "./server/birdnet-api/run.sh",
    "express-api": "npm run dev --workspace server/express-api",
    "birdnet-client": "npm run start --workspace birdnet-client"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
