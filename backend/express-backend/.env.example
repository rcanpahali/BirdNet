# URL of the BirdNET FastAPI backend
BIRDNET_API_URL=http://localhost:8000
# Express server port
PORT=8080
# Optional upload limit (bytes). Defaults to 104857600 (100MB) when unset
MAX_FILE_SIZE=104857600
# Optional SQLite database path (relative paths resolve from project root)
DATABASE_PATH=./data/birdnet.db
